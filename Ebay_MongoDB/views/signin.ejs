								<div class="col-xs-6 text-right wow fadeInUp"
									data-wow-delay="1s">
									<a href="#/login" class="btn btn-secondary">Login</a>
								</div>
								<div class="col-xs-6 text-left wow fadeInUp"
									data-wow-delay="1.4s">
									<a href="#/register" class="btn btn-primary">Register User</a>
								</div>
							</div>
						</div>
					</div>

				</div>
			</div>
